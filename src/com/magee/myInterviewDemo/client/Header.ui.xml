<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.border {
		border: 2px solid black;
	}
	.link {
		margin-top: 7%;
    display:inline-block;
	}
	.link:hover {
	 cursor:pointer;
     color:blue;
     text-decoration:underline;
     text-decoration:none;
     text-shadow: 1px 1px 1px #555;
	}
	</ui:style>
	<g:HTMLPanel styleName='{style.border}'>
	    <div class="row">
	        <div class="col-6">
	            <h1 style="padding-left: 5%; padding-top: 2%">Matthew Magee</h1>
	        </div>
	        <div class="col-6">
		    	<g:Anchor styleName='{style.link}' ui:field="home"></g:Anchor>
		    	<g:Anchor styleName='{style.link}' ui:field="printing"></g:Anchor>
		    	<g:Anchor styleName='{style.link}' ui:field="robotics"></g:Anchor>
	    	</div>
	    </div>
	</g:HTMLPanel>
</ui:UiBinder> 